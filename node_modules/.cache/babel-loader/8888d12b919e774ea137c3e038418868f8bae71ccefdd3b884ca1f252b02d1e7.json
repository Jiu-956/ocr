ib from \"pdfjs-dist\";\r\nimport pdfjsWorker from \"pdfjs-dist/build/pdf.worker?url\";\r\n\r\npdfjsLib.GlobalWorkerOptions.workerSrc = pdfjsWorker;\r\n\r\n\r\n\r\nexport default {\r\n  name: \"OcrResults\",\r\n  computed: {\r\n    ...mapState([\"ocrResults\"]),\r\n  },\r\n  data() {\r\n    return {\r\n      pdfDoc: null,\r\n      pdfPages: [],\r\n      latexLines: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    if (this.ocrResults) {\r\n      // 加载 LaTeX\r\n      if (this.ocrResults.latex) {\r\n        this.latexLines = this.ocrResults.latex.split(\"\\n\");\r\n      }\r\n\r\n      // 加载 PDF\r\n      if (this.ocrResults.pdfUrl) {\r\n        this.loadPDF(this.ocrResults.pdfUrl);\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    async loadPDF(url) {\r\n      const loadingTask = pdfjsLib.getDocument(url);\r\n      this.pdfDoc = await loadingTask.promise;\r\n      const numPages = this.pdfDoc.numPages;\r\n\r\n      for (let i = 1; i <= numPages; i++) {\r\n        const page = await this.pdfDoc.getPage(i);\r\n        this.pdfPages.push(page);\r\n        this.renderPage(i, page);\r\n      }\r\n    },\r\n    async renderPage(pageNum, page) {\r\n      const container = this.$refs[\"page-\" + (pageNum - 1)][0];\r\n      const viewport = page.getViewport({ scale: 1.2 });\r\n      const canvas = document.createElement(\"canvas\");\r\n      canvas.width = viewport.width;\r\n      canvas.height = viewport.height;\r\n      container.appendChild(canvas);\r\n\r\n      const context = canvas.getContext(\"2d\");\r\n      const renderContext = { canvasContext: context, viewport: viewport };\r\n      await page.render(renderContext).promise;\r\n    },\r\n    scrollToPage(index) {\r\n      // 简单映射：每行 LaTeX 对应 PDF 的一页（可根据需要改映射规则）\r\n      const pageEl = this.$refs[\"page-\" + index]?.[0];\r\n      const container = this.$refs.pdfContainer;\r\n\r\n      if (pageEl) {\r\n        container.scrollTop = pageEl.offsetTop;\r\n        pageEl.style.border = \"2px solid red\";\r\n      }\r\n    },\r\n    clearHighlight(index) {\r\n      const pageEl = this.$refs[\"page-\" + index]?.[0];\r\n      if (pageEl) {\r\n        pageEl.style.border = \"none\";\r\n      }\r\n    },\r\n    goBack() {\r\n      this.$store.commit(\"resetState\");\r\n      this.$router.push(\"/\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.results-container {\r\n  display: flex;\r\n  gap: 20px;\r\n  margin: 2rem 0;\r\n  height: 80vh;\r\n}\r\n\r\n/* 左侧 PDF */\r\n.pdf-container {\r\n  flex: 1;\r\n  overflow-y: auto;\r\n  background: #eee;\r\n  padding: 10px;\r\n  border-radius: 6px;\r\n}\r\n.pdf-page {\r\n  margin-bottom: 10px;\r\n  position: relative;\r\n}\r\n\r\n/* 右侧 LaTeX */\r\n.latex-container {\r\n  flex: 1;\r\n  overflow-y: auto;\r\n  padding: 10px;\r\n  background: #f9f9f9;\r\n  border-radius: 6px;\r\n  border-left: 1px solid #ccc;\r\n}\r\n.latex-line {\r\n  padding: 5px;\r\n  cursor: pointer;\r\n}\r\n.latex-line:hover {\r\n  background-color: #d3f3ff;\r\n}\r\n\r\n.no-results {\r\n  margin: 2rem 0;\r\n  color: #666;\r\n}\r\n.back-button {\r\n  margin-top: 2rem;\r\n  background-color: #42b983;\r\n  color: white;\r\n  border: none;\r\n  padding: 0.75rem 1.5rem;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n}\r\n.back-button:hover {\r\n  background-color: #3aa876;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAS;;;EAGKA,KAAK,EAAC;;;EAEtBA,KAAK,EAAC,eAAe;EAACC,GAAG,EAAC;;;EAU1BD,KAAK,EAAC;AAAiB;;;;EAclBA,KAAK,EAAC;;;uBA7BpBE,mBAAA,CAkCM,OAlCNC,UAkCM,G,0BAjCJC,mBAAA,CAAoB,YAAhB,aAAW,qBAEJC,IAAA,CAAAC,UAAU,I,cAArBJ,mBAAA,CAwBM,OAxBNK,UAwBM,GAvBJC,mBAAA,eAAkB,EAClBJ,mBAAA,CAOM,OAPNK,UAOM,I,kBANJP,mBAAA,CAKOQ,SAAA,QAAAC,WAAA,CAJmBN,IAAA,CAAAO,QAAQ,GAAxBC,IAAI,EAAEC,KAAK;yBADrBZ,mBAAA,CAKO;MAHJa,GAAG,EAAED,KAAK;MACXd,KAAK,EAAC,UAAU;;MACfC,GAAG,YAAYa;;0DAIpBN,mBAAA,yBAA4B,EAC5BJ,mBAAA,CAWM,OAXNY,UAWM,G,0BAVJZ,mBAAA,CAAyB,YAArB,kBAAgB,sB,kBACpBF,mBAAA,CAQMQ,SAAA,QAAAC,WAAA,CAPoBN,IAAA,CAAAY,UAAU,GAA1BC,IAAI,EAAEJ,KAAK;yBADrBZ,mBAAA,CAQM;MA