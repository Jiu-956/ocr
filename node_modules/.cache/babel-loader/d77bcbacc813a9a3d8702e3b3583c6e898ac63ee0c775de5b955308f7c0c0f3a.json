{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { mapState } from 'vuex';\nimport * as pdfjsLib from 'pdfjs-dist';\nexport default {\n  name: 'OcrResults',\n  computed: {\n    ...mapState(['ocrResults']),\n    hasData() {\n      // ÈúÄË¶ÅÔºöocrResults.file ‰∏∫ FileÔºõlatex ‰∏∫Êï∞ÁªÑÊàñÊñáÊú¨\n      return !!(this.ocrResults && this.ocrResults.file && (this.ocrResults.latex || this.ocrResults.text));\n    }\n  },\n  data() {\n    return {\n      pdfDoc: null,\n      pageViews: [],\n      // { pageNumber, viewport } ÂàóË°®\n      canvasRefs: [],\n      // canvas DOM refs\n      pageRefs: [],\n      // ÊØèÈ°µÂÆπÂô® DOM refs\n      activePageIndex: -1,\n      // ÂΩìÂâçÈ´ò‰∫ÆÈ°µ\n      latexLines: [],\n      // Ê∏≤ÊüìÂà∞Âè≥‰æßÁöÑ HTML Ë°å\n      lineToPageMap: [] // Ë°å -> È°µÁ¥¢Âºï ÁöÑÊò†Â∞Ñ\n    };\n  },\n  async mounted() {\n    if (!this.hasData) return;\n\n    // 1) ËΩΩÂÖ• PDFÔºà‰∏ç‰ΩøÁî® pdf_viewer.cssÔºõÁ¶ÅÁî® worker ËßÑÈÅøÊâìÂåÖÈóÆÈ¢òÔºâ\n    await this.loadPDF(this.ocrResults.file);\n\n    // 2) ÂáÜÂ§á LaTeX Ë°åÔºà‰ºòÂÖà‰ΩøÁî® ocrResults.latexÔºõÂê¶Âàô‰ªé text ÊåâË°åÊãÜÂàÜÔºâ\n    const rawLatex = Array.isArray(this.ocrResults.latex) ? this.ocrResults.latex : typeof this.ocrResults.latex === 'string' ? this.ocrResults.latex.split('\\n') : this.ocrResults.text ? String(this.ocrResults.text).split('\\n') : [];\n    await this.loadLatex(rawLatex);\n\n    // 3) ÂáÜÂ§áË°å->È°µÊò†Â∞ÑÔºàÂ¶ÇÊûúÂêéÁ´ØÁªô‰∫Ü mapping Â∞±Áî®ÔºåÊ≤°ÊúâÂàôÁî®Á≠âÂàÜËøë‰ººÔºâ\n    this.buildLineToPageMap(rawLatex.length);\n  },\n  methods: {\n    goBack() {\n      this.$store.commit('resetState');\n      this.$router.push('/');\n    },\n    // Âä†ËΩΩÂπ∂Ê∏≤ÊüìÊï¥‰ªΩ PDFÔºàÈÄêÈ°µÁîªÂà∞ canvasÔºâ\n    async loadPDF(file) {\n      try {\n        const buffer = await file.arrayBuffer();\n        const loadingTask = pdfjsLib.getDocument({\n          data: buffer,\n          disableWorker: true // üö´ ‰∏ç‰ΩøÁî® workerÔºåÈÅøÂÖçÈ¢ùÂ§ñÊâìÂåÖÈÖçÁΩÆ\n        });\n        this.pdfDoc = await loadingTask.promise;\n        this.pageViews = Array.from({\n          length: this.pdfDoc.numPages\n        }, (_, i) => ({\n          pageNumber: i + 1,\n          viewport: null\n        }));\n\n        // È°∫Â∫èÊ∏≤ÊüìÊØè‰∏ÄÈ°µ\n        for (let i = 0; i < this.pageViews.length; i++) {\n          await this.renderPage(i);\n        }\n      } catch (err) {\n        console.error('Âä†ËΩΩ/Ê∏≤Êüì PDF Âá∫ÈîôÔºö', err);\n      }\n    },\n    async renderPage(index) {\n      const pageNumber = index + 1;\n      const page = await this.pdfDoc.getPage(pageNumber);\n      const viewport = page.getViewport({\n        scale: 1.5\n      });\n      this.pageViews[index].viewport = viewport;\n      const canvas = this.canvasRefs[index];\n      const ctx = canvas.getContext('2d');\n      canvas.width = Math.ceil(viewport.width);\n      canvas.height = Math.ceil(viewport.height);\n      await page.render({\n        canvasContext: ctx,\n        viewport\n      }).promise;\n    },\n    // Áî® MathJax Ê∏≤Êüì LaTeX Ë°åÔºõÂ¶ÇÊûúÊ≤°Âä†ËΩΩ MathJaxÔºåÂàôÈÄÄÂåñ‰∏∫ÂéüÊñá\n    async loadLatex(lines) {\n      const mj = window.MathJax;\n      if (!mj || !mj.tex2chtml) {\n        // ÈÄÄÂåñÔºö‰∏çÊ∏≤ÊüìÔºåÁõ¥Êé•ÊòæÁ§∫ÂéüÊñáÊú¨\n        this.latexLines = lines.map(l => this.escapeHtml(l)).map(html => `<pre class=\"plain\">${html}</pre>`);\n        return;\n      }\n      this.latexLines = lines.map(line => mj.tex2chtml(line).outerHTML);\n      // ËÆ© MathJax ÈÅçÂéÜ typesetÔºàÈò≤Ê≠¢ÂàùÊ¨°ÊèíÂÖ•‰∏çÂ∏ÉÂ±ÄÔºâ\n      await mj.typesetPromise?.();\n    },\n    // Ë°å -> È°µ Êò†Â∞Ñ\n    buildLineToPageMap(totalLines) {\n      // Â¶ÇÊûú‰Ω†ÁöÑÂêéÁ´ØËÉΩËøîÂõûÁ≤æÁ°ÆÊò†Â∞ÑÔºà‰æãÂ¶ÇÔºöÊØèË°åÂ∏¶ pageIndexÔºâÔºåÂèØ‰ª•ÊîæÂà∞ ocrResults.linePageMap\n      if (Array.isArray(this.ocrResults.linePageMap) && this.ocrResults.linePageMap.length === totalLines) {\n        this.lineToPageMap = this.ocrResults.linePageMap;\n        return;\n      }\n\n      // Ê≤°ÊúâÊò†Â∞ÑÂ∞±ÂùáÂåÄÁ≠âÂàÜÔºöÊääË°åÊåâÈ°µÂπ≥ÂùáÂàÜÈÖçÔºàÈ°µÁ∫ßÂÆö‰ΩçÔºâ\n      const pages = this.pageViews.length || 1;\n      this.lineToPageMap = Array.from({\n        length: totalLines\n      }, (_, i) => {\n        const pageIdx = Math.min(pages - 1, Math.floor(i * pages / Math.max(1, totalLines)));\n        return pageIdx;\n      });\n    },\n    // ÊÇ¨ÂÅúËÅîÂä®ÔºöÊªöÂä®Âà∞ÂØπÂ∫îÈ°µÂπ∂È´ò‰∫ÆÈ°µÊ°Ü\n    highlightLine(lineIndex) {\n      const pageIdx = this.lineToPageMap[lineIndex] ?? 0;\n      const pageEl = this.pageRefs[pageIdx];\n      if (!pageEl) return;\n\n      // ÊªöÂä®Âà∞ËØ•È°µÔºàÂ±Ö‰∏≠Ôºâ\n      pageEl.scrollIntoView({\n        behavior: 'smooth',\n        block: 'center'\n      });\n\n      // È´ò‰∫ÆËØ•È°µ\n      this.activePageIndex = pageIdx;\n    },\n    clearHighlight() {\n      this.activePageIndex = -1;\n    },\n    escapeHtml(s) {\n      return String(s).replaceAll('&', '&amp;').replaceAll('<', '&lt;').replaceAll('>', '&gt;');\n    }\n  }\n};","map":{"version":3,"names":["mapState","pdfjsLib","name","computed","hasData","ocrResults","file","latex","text","data","pdfDoc","pageViews","canvasRefs","pageRefs","activePageIndex","latexLines","lineToPageMap","mounted","loadPDF","rawLatex","Array","isArray","split","String","loadLatex","buildLineToPageMap","length","methods","goBack","$store","commit","$router","push","buffer","arrayBuffer","loadingTask","getDocument","disableWorker","promise","from","numPages","_","i","pageNumber","viewport","renderPage","err","console","error","index","page","getPage","getViewport","scale","canvas","ctx","getContext","width","Math","ceil","height","render","canvasContext","lines","mj","window","MathJax","tex2chtml","map","l","escapeHtml","html","line","outerHTML","typesetPromise","totalLines","linePageMap","pages","pageIdx","min","floor","max","highlightLine","lineIndex","pageEl","scrollIntoView","behavior","block","clearHighlight","s","replaceAll"],"sources":["D:\\OCR\\ocr\\src\\views\\Results.vue"],"sourcesContent":["<template>\r\n  <div class=\"results\">\r\n    <h2>OCR Results</h2>\r\n\r\n    <div v-if=\"hasData\" class=\"results-container\">\r\n      <div class=\"split\">\r\n        <!-- Â∑¶ÔºöPDF Ê∏≤ÊüìÔºàÊØèÈ°µ‰∏Ä‰∏™ canvasÔºâ -->\r\n        <div class=\"pdf-pane\" ref=\"pdfPane\">\r\n          <div\r\n            v-for=\"(page, i) in pageViews\"\r\n            :key=\"i\"\r\n            class=\"pdf-page\"\r\n            :class=\"{ active: i === activePageIndex }\"\r\n            :ref=\"el => pageRefs[i] = el\"\r\n          >\r\n            <canvas :ref=\"el => canvasRefs[i] = el\"></canvas>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Âè≥ÔºöLaTeX Ë°åÂàóË°®ÔºàMathJax Ê∏≤ÊüìÔºâ -->\r\n        <div class=\"latex-pane\" ref=\"latexPane\">\r\n          <div\r\n            v-for=\"(line, idx) in latexLines\"\r\n            :key=\"idx\"\r\n            class=\"latex-line\"\r\n            @mouseenter=\"highlightLine(idx)\"\r\n            @mouseleave=\"clearHighlight\"\r\n            v-html=\"line\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else class=\"no-results\">\r\n      <p>ÊöÇÊó†ÂèØÊ∏≤ÊüìÁöÑÊï∞ÊçÆ„ÄÇËØ∑ÂÖà‰∏ä‰º† PDF Âπ∂Á°Æ‰øù Vuex ÁöÑ ocrResults ‰∏≠Êúâ <code>file</code>ÔºàFile ÂØπË±°ÔºâÂíå <code>latex</code>ÔºàÊï∞ÁªÑÊàñÊñáÊú¨Ôºâ„ÄÇ</p>\r\n    </div>\r\n\r\n    <button @click=\"goBack\" class=\"back-button\">Process Another File</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex'\r\nimport * as pdfjsLib from 'pdfjs-dist'\r\n\r\nexport default {\r\n  name: 'OcrResults',\r\n  computed: {\r\n    ...mapState(['ocrResults']),\r\n    hasData() {\r\n      // ÈúÄË¶ÅÔºöocrResults.file ‰∏∫ FileÔºõlatex ‰∏∫Êï∞ÁªÑÊàñÊñáÊú¨\r\n      return !!(this.ocrResults && this.ocrResults.file && (this.ocrResults.latex || this.ocrResults.text))\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      pdfDoc: null,\r\n      pageViews: [],              // { pageNumber, viewport } ÂàóË°®\r\n      canvasRefs: [],             // canvas DOM refs\r\n      pageRefs: [],               // ÊØèÈ°µÂÆπÂô® DOM refs\r\n      activePageIndex: -1,        // ÂΩìÂâçÈ´ò‰∫ÆÈ°µ\r\n      latexLines: [],             // Ê∏≤ÊüìÂà∞Âè≥‰æßÁöÑ HTML Ë°å\r\n      lineToPageMap: []           // Ë°å -> È°µÁ¥¢Âºï ÁöÑÊò†Â∞Ñ\r\n    }\r\n  },\r\n  async mounted() {\r\n    if (!this.hasData) return\r\n\r\n    // 1) ËΩΩÂÖ• PDFÔºà‰∏ç‰ΩøÁî® pdf_viewer.cssÔºõÁ¶ÅÁî® worker ËßÑÈÅøÊâìÂåÖÈóÆÈ¢òÔºâ\r\n    await this.loadPDF(this.ocrResults.file)\r\n\r\n    // 2) ÂáÜÂ§á LaTeX Ë°åÔºà‰ºòÂÖà‰ΩøÁî® ocrResults.latexÔºõÂê¶Âàô‰ªé text ÊåâË°åÊãÜÂàÜÔºâ\r\n    const rawLatex = Array.isArray(this.ocrResults.latex)\r\n      ? this.ocrResults.latex\r\n      : (typeof this.ocrResults.latex === 'string'\r\n          ? this.ocrResults.latex.split('\\n')\r\n          : (this.ocrResults.text ? String(this.ocrResults.text).split('\\n') : []))\r\n\r\n    await this.loadLatex(rawLatex)\r\n\r\n    // 3) ÂáÜÂ§áË°å->È°µÊò†Â∞ÑÔºàÂ¶ÇÊûúÂêéÁ´ØÁªô‰∫Ü mapping Â∞±Áî®ÔºåÊ≤°ÊúâÂàôÁî®Á≠âÂàÜËøë‰ººÔºâ\r\n    this.buildLineToPageMap(rawLatex.length)\r\n  },\r\n  methods: {\r\n    goBack() {\r\n      this.$store.commit('resetState')\r\n      this.$router.push('/')\r\n    },\r\n\r\n    // Âä†ËΩΩÂπ∂Ê∏≤ÊüìÊï¥‰ªΩ PDFÔºàÈÄêÈ°µÁîªÂà∞ canvasÔºâ\r\n    async loadPDF(file) {\r\n      try {\r\n        const buffer = await file.arrayBuffer()\r\n        const loadingTask = pdfjsLib.getDocument({\r\n          data: buffer,\r\n          disableWorker: true // üö´ ‰∏ç‰ΩøÁî® workerÔºåÈÅøÂÖçÈ¢ùÂ§ñÊâìÂåÖÈÖçÁΩÆ\r\n        })\r\n        this.pdfDoc = await loadingTask.promise\r\n\r\n        this.pageViews = Array.from({ length: this.pdfDoc.numPages }, (_, i) => ({\r\n          pageNumber: i + 1,\r\n          viewport: null\r\n        }))\r\n\r\n        // È°∫Â∫èÊ∏≤ÊüìÊØè‰∏ÄÈ°µ\r\n        for (let i = 0; i < this.pageViews.length; i++) {\r\n          await this.renderPage(i)\r\n        }\r\n      } catch (err) {\r\n        console.error('Âä†ËΩΩ/Ê∏≤Êüì PDF Âá∫ÈîôÔºö', err)\r\n      }\r\n    },\r\n\r\n    async renderPage(index) {\r\n      const pageNumber = index + 1\r\n      const page = await this.pdfDoc.getPage(pageNumber)\r\n      const viewport = page.getViewport({ scale: 1.5 })\r\n      this.pageViews[index].viewport = viewport\r\n\r\n      const canvas = this.canvasRefs[index]\r\n      const ctx = canvas.getContext('2d')\r\n      canvas.width = Math.ceil(viewport.width)\r\n      canvas.height = Math.ceil(viewport.height)\r\n\r\n      await page.render({ canvasContext: ctx, viewport }).promise\r\n    },\r\n\r\n    // Áî® MathJax Ê∏≤Êüì LaTeX Ë°åÔºõÂ¶ÇÊûúÊ≤°Âä†ËΩΩ MathJaxÔºåÂàôÈÄÄÂåñ‰∏∫ÂéüÊñá\r\n    async loadLatex(lines) {\r\n      const mj = window.MathJax\r\n      if (!mj || !mj.tex2chtml) {\r\n        // ÈÄÄÂåñÔºö‰∏çÊ∏≤ÊüìÔºåÁõ¥Êé•ÊòæÁ§∫ÂéüÊñáÊú¨\r\n        this.latexLines = lines.map(l => this.escapeHtml(l)).map(html => `<pre class=\"plain\">${html}</pre>`)\r\n        return\r\n      }\r\n      this.latexLines = lines.map(line => mj.tex2chtml(line).outerHTML)\r\n      // ËÆ© MathJax ÈÅçÂéÜ typesetÔºàÈò≤Ê≠¢ÂàùÊ¨°ÊèíÂÖ•‰∏çÂ∏ÉÂ±ÄÔºâ\r\n      await mj.typesetPromise?.()\r\n    },\r\n\r\n    // Ë°å -> È°µ Êò†Â∞Ñ\r\n    buildLineToPageMap(totalLines) {\r\n      // Â¶ÇÊûú‰Ω†ÁöÑÂêéÁ´ØËÉΩËøîÂõûÁ≤æÁ°ÆÊò†Â∞ÑÔºà‰æãÂ¶ÇÔºöÊØèË°åÂ∏¶ pageIndexÔºâÔºåÂèØ‰ª•ÊîæÂà∞ ocrResults.linePageMap\r\n      if (Array.isArray(this.ocrResults.linePageMap) && this.ocrResults.linePageMap.length === totalLines) {\r\n        this.lineToPageMap = this.ocrResults.linePageMap\r\n        return\r\n      }\r\n\r\n      // Ê≤°ÊúâÊò†Â∞ÑÂ∞±ÂùáÂåÄÁ≠âÂàÜÔºöÊääË°åÊåâÈ°µÂπ≥ÂùáÂàÜÈÖçÔºàÈ°µÁ∫ßÂÆö‰ΩçÔºâ\r\n      const pages = this.pageViews.length || 1\r\n      this.lineToPageMap = Array.from({ length: totalLines }, (_, i) => {\r\n        const pageIdx = Math.min(pages - 1, Math.floor(i * pages / Math.max(1, totalLines)))\r\n        return pageIdx\r\n      })\r\n    },\r\n\r\n    // ÊÇ¨ÂÅúËÅîÂä®ÔºöÊªöÂä®Âà∞ÂØπÂ∫îÈ°µÂπ∂È´ò‰∫ÆÈ°µÊ°Ü\r\n    highlightLine(lineIndex) {\r\n      const pageIdx = this.lineToPageMap[lineIndex] ?? 0\r\n      const pageEl = this.pageRefs[pageIdx]\r\n      if (!pageEl) return\r\n\r\n      // ÊªöÂä®Âà∞ËØ•È°µÔºàÂ±Ö‰∏≠Ôºâ\r\n      pageEl.scrollIntoView({ behavior: 'smooth', block: 'center' })\r\n\r\n      // È´ò‰∫ÆËØ•È°µ\r\n      this.activePageIndex = pageIdx\r\n    },\r\n\r\n    clearHighlight() {\r\n      this.activePageIndex = -1\r\n    },\r\n\r\n    escapeHtml(s) {\r\n      return String(s)\r\n        .replaceAll('&', '&amp;')\r\n        .replaceAll('<', '&lt;')\r\n        .replaceAll('>', '&gt;')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.results {\r\n  padding: 1rem;\r\n}\r\n\r\n.results-container {\r\n  margin-top: 1rem;\r\n}\r\n\r\n/* ‰∏§Ê†èÂ∏ÉÂ±Ä */\r\n.split {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  gap: 12px;\r\n  height: 80vh;\r\n}\r\n\r\n/* Â∑¶‰æß PDF */\r\n.pdf-pane {\r\n  overflow: auto;\r\n  background: #f5f7fa;\r\n  padding: 8px;\r\n  border: 1px solid #e5e7eb;\r\n  border-radius: 8px;\r\n}\r\n\r\n.pdf-page {\r\n  margin: 8px auto;\r\n  padding: 4px;\r\n  border: 2px solid transparent;\r\n  border-radius: 6px;\r\n  width: max-content;\r\n  max-width: 100%;\r\n}\r\n\r\n.pdf-page.active {\r\n  border-color: #ef4444; /* ÊÇ¨ÂÅúÂØπÂ∫îÈ°µÈ´ò‰∫ÆÁ∫¢Ê°Ü */\r\n  box-shadow: 0 0 0 4px rgba(239, 68, 68, 0.15);\r\n}\r\n\r\n/* Âè≥‰æß LaTeX Âå∫ */\r\n.latex-pane {\r\n  overflow: auto;\r\n  padding: 8px 12px;\r\n  border: 1px solid #e5e7eb;\r\n  border-radius: 8px;\r\n  background: #fff;\r\n}\r\n\r\n.latex-line {\r\n  padding: 6px 8px;\r\n  border-radius: 6px;\r\n  cursor: default;\r\n}\r\n\r\n.latex-line:hover {\r\n  background: #fff7cc;\r\n}\r\n\r\n/* Á∫ØÊñáÊú¨ÈôçÁ∫ßÊ†∑Âºè */\r\npre.plain {\r\n  margin: 0;\r\n  white-space: pre-wrap;\r\n  word-break: break-word;\r\n  font-family: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace;\r\n  font-size: 14px;\r\n}\r\n\r\n/* ÂÖ∂ÂÆÉ */\