{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapState } from 'vuex';\nexport default {\n  name: 'OcrResults',\n  computed: {\n    ...mapState(['ocrResults'])\n  },\n  methods: {\n    goBack() {\n      this.$store.commit('resetState');\n      this.$router.push('/');\n    }\n  }\n};","map":{"version":3,"names":["mapState","name","computed","methods","goBack","$store","commit","$router","push"],"sources":["D:\\OCR\\ocr\\src\\views\\Results.vue"],"sourcesContent":["<template>\r\n  <div class=\"results\">\r\n    <h2>OCR Results</h2>\r\n    \r\n    <div v-if=\"ocrResults\" class=\"results-container\">\r\n      <div class=\"result-section\">\r\n        <h3>Extracted Text:</h3>\r\n        <pre class=\"extracted-text\">{{ ocrResults.text }}</pre>\r\n      </div>\r\n      \r\n      <div v-if=\"ocrResults.pages\" class=\"result-section\">\r\n        <h3>Page Details:</h3>\r\n        <div v-for=\"(page, index) in ocrResults.pages\" :key=\"index\" class=\"page-details\">\r\n          <h4>Page {{ index + 1 }}</h4>\r\n          <p>Confidence: {{ page.confidence.toFixed(2) }}%</p>\r\n          <pre v-if=\"page.text\">{{ page.text }}</pre>\r\n        </div>\r\n      </div>\r\n      \r\n      <div v-if=\"ocrResults.image\" class=\"result-section\">\r\n        <h3>Processed Image:</h3>\r\n        <img :src=\"`data:image/jpeg;base64,${ocrResults.image}`\" alt=\"Processed document\" class=\"processed-image\">\r\n      </div>\r\n    </div>\r\n    \r\n    <div v-else class=\"no-results\">\r\n      <p>No results to display.</p>\r\n    </div>\r\n    \r\n    <button @click=\"goBack\" class=\"back-button\">Process Another File</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex'\r\n\r\nexport default {\r\n  name: 'OcrResults',\r\n  computed: {\r\n    ...mapState(['ocrResults'])\r\n  },\r\n  methods: {\r\n    goBack() {\r\n      this.$store.commit('resetState')\r\n      this.$router.push('/')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\